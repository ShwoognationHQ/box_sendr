<h1>Authed?</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Details</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% [@box_login].compact.each do |box_login| %>
      <tr>
        <td><%= box_login.name %></td>
        <td><%= box_login.details %></td>
        <td><%= link_to 'Show', box_login %></td>
        <td><%= link_to 'Edit', edit_box_login_path(box_login) %></td>
        <td><%= link_to 'Destroy', box_login, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

